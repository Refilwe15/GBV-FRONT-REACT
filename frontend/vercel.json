{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run web",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "EXPO_USE_FAST_RESOLVER": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "EXPO_NO_TELEMETRY": "1"
    }
  }
}
