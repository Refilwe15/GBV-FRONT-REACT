{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "installCommand": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
  "devCommand": "npm run web",
  "env": {
    "EXPO_USE_FAST_RESOLVER": "1",
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
